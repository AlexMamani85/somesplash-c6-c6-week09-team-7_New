<section class="section-sm expand">
  <div class="container-sm">
    <div class="search">
      <h1 class="head-md semibold">Search</h1>
      <div class="search">
        <%= form_with url: "/search", method: :get do |form| %>
            <% form.label :query %>
            <%= form.text_field :query, placeholder: "search", class:"search input" %>
            <%= form.submit "SEARCH PHOTOS", class:"index-button button button--secondary btn-width100 overline b-16"  %>
          <% end %>
      </div>
    </div>
  </div>
</section>
